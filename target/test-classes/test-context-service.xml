<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns = "http://www.springframework.org/schema/beans"
       xmlns:xsi = "http://www.w3.org/2001/XMLSchema-instance"
       xsi:schemaLocation = "http://www.springframework.org/schema/beans 
                             http://www.springframework.org/schema/beans/spring-beans.xsd">

    <bean id = "catalogService" 
          name = "catalogService" 
          class = "org.mockito.Mockito" 
          factory-method = "mock">
        <constructor-arg value="com.yorku.Hajus.model.Service.CatalogService"/>
    </bean>
    
    <bean id = "documentService" 
          name = "documentService" 
          class = "org.mockito.Mockito" 
          factory-method = "mock">
        <constructor-arg value="com.yorku.Hajus.model.Service.DocumentService"/>
    </bean>
</beans>